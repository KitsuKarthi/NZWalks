@model IEnumerable<NZWalks.UI.Models.DTO.RegionDto>

@{

}

<h1 class="mt-3">Regions</h1>

<table class="table table-bordered">

    <thead>
        <tr>
            <th>ID</th>
            <th>Code</th>
            <th>Name</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var region in Model)
        {
            <tr>
                <th>@region.Id</th>
                <th>@region.code</th>
                <th>@region.name</th>
            </tr>
        }
    </tbody>

</table>